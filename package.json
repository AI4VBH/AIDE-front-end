{
  "name": "aide-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "serve:test": "cross-env VUE_APP_AUTH_ENABLED=false && npm run serve",
    "serve:cypress": "cross-env VUE_APP_AUTH_ENABLED=false vue-cli-service serve --mode production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "tests:ui_headless": "cypress run -b chrome",
    "tests:ui_debug": "npx cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress open",
    "serve:test:debug": "npm run serve:cypress && npm run tests:ui_debug",
    "tests:ui_ci": "start-server-and-test serve:cypress http://localhost:8080 tests:ui_headless",
    "tests:ui_browser": "start-server-and-test serve:cypress http://localhost:8080 tests:ui_debug",
    "tests:consumer": "cross-env TS_NODE_PROJECT=\"pact/tsconfig.test.json\" mocha --timeout 10000 -r ts-node/register pact/test/*.spec.ts",
    "pact:publish": "pact-broker publish pact/pacts --consumer-app-version=\"$(npx @pact-foundation/absolute-version)\" --tag-with-git-branch --broker-base-url=http://ec2-3-9-59-106.eu-west-2.compute.amazonaws.com:9292/"
  },
  "dependencies": {
    "@ssthouse/vue-tree-chart": "^0.6.7",
    "axios": "^0.27.2",
    "chart.js": "^3.8.0",
    "core-js": "^3.6.5",
    "cornerstone-core": "^2.6.0",
    "cornerstone-math": "^0.1.9",
    "cornerstone-tools": "^6.0.0",
    "cornerstone-web-image-loader": "^2.1.1",
    "hammerjs": "^2.0.8",
    "keycloak-js": "^15.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "numeral": "^2.0.6",
    "pdfvuer": "^1.9.2",
    "underscore": "^1.13.4",
    "vue": "^2.6.11",
    "vue-chartjs": "^4.1.1",
    "vue-class-component": "^7.2.3",
    "vue-json-pretty": "^1.8.1",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-toast-notification": "^0.6.2",
    "vue2-filters": "^0.13.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@pact-foundation/absolute-version": "^0.0.4",
    "@pact-foundation/pact": "^9.17.1",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/jest": "^24.0.19",
    "@types/mochawesome": "^6.2.0",
    "@types/node": "^15.12.0",
    "@types/numeral": "^2.0.2",
    "@types/sinon-chai": "^3.2.8",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vue/cli-plugin-babel": "~5.0",
    "@vue/cli-plugin-eslint": "~5.0",
    "@vue/cli-plugin-router": "~5.0",
    "@vue/cli-plugin-typescript": "~5.0",
    "@vue/cli-plugin-unit-jest": "~5.0",
    "@vue/cli-plugin-vuex": "~5.0",
    "@vue/cli-service": "~5.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^1.0.3",
    "axe-core": "^4.3.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^10.7.0",
    "cypress-axe": "^1.0.0",
    "cypress-get-table": "^1.0.1",
    "eslint": "^8.23.0",
    "eslint-plugin-prettier": "^4.0",
    "eslint-plugin-vue": "^9.0",
    "keycloak-mock": "^1.0.10",
    "mocha": "^8.4.0",
    "mochawesome": "^6.2.2",
    "msw": "^0.46.1",
    "prettier": "^2.2.1",
    "sass": "~1.32.12",
    "sass-loader": "^10.0.0",
    "scroll-to-bottomjs": "^1.1.0",
    "sinon-chai": "^3.7.0",
    "start-server-and-test": "^1.13.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.5",
    "vue-cli-plugin-vuetify": "^2.4.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
