    "keycloak-js": "^19.0.3",
    "mime-types": "^2.1.35",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "numeral": "^2.0.6",
    "pdfvuer": "^1.9.2",
    "underscore": "^1.13.4",
    "vanilla-jsoneditor": "^0.9.1",
    "vue": "^2.6.11",
    "vue-chartjs": "^4.1.1",
    "vue-class-component": "^7.2.3",
