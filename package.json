{
  "name": "aide-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "serve:test": "cross-env VUE_APP_AUTH_ENABLED=false && npm run serve",
    "serve:cypress": "cross-env VUE_APP_AUTH_ENABLED=false vue-cli-service serve --mode production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "audit": "better-npm-audit audit -l moderate -p",
    "tests:ui_headless": "cypress run -b chrome",
    "tests:ui_components": "cypress run --component -b chrome",
    "tests:ui_debug": "npx cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress open",
    "serve:test:debug": "npm run serve:cypress && npm run tests:ui_debug",
    "tests:ui_ci": "start-server-and-test serve:cypress http://localhost:8080 tests:ui_headless",
    "tests:ui_browser": "start-server-and-test serve:cypress http://localhost:8080 tests:ui_debug"
  },
  "dependencies": {
